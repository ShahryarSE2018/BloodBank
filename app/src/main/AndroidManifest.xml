<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shahryarahmed.onlinebloodbank">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Main.Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login.Login" />
        <activity android:name=".User.User" />
        <activity android:name=".Staff.Staff" />
        <activity android:name=".Registration.UserRegistration" />
        <activity android:name=".Registration.StaffRegistration" />
        <activity android:name=".BloodBankData.Data"></activity>
    </application>

</manifest>